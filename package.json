{
  "name": "@supergrowthai/next-blog",
  "version": "1.0.13",
  "description": "Add blogging to your nextjs project in a jiffy",
  "repository": "https://github.com/captadexp/next-blog",
  "type": "module",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf *.js *.d.ts* adapters components pages utils"
  },
  "keywords": [
    "nextjs",
    "react",
    "cms",
    "blogging",
    "headless-cms",
    "next-js",
    "secure"
  ],
  "files": [
    "./*.js",
    "./*.d.ts*",
    "./**/*.js",
    "./**/*.d.ts*"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./adapters/*": {
      "types": "./adapters/*.d.ts",
      "import": "./adapters/*.js"
    },
    "./ui": {
      "types": "./ui/index.d.ts",
      "import": "./ui/index.js"
    },
    "./types": {
      "types": "./types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "author": "Capt ADExp <tech@developer.toys>",
  "license": "ISC",
  "dependencies": {
    "moment": "^2.30.1",
    "preact": "10.11.3",
    "preact-render-to-string": "5.2.3"
  },
  "optionalDependencies": {
    "mongodb": "^6.5.0",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "next": "^15.1.0"
  },
  "devDependencies": {
    "@types/mongodb": "^4.0.7",
    "@types/uuid": "^9.0.8"
  }
}
