{
  "name": "next-blog-workspace",
  "version": "1.0.0",
  "description": "Workspace for next-blog packages",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run -w packages/core build",
    "dev": "npm run -w packages/core dev",
    "dev:test": "npm run -w packages/test-app dev",
    "build:test": "npm run -w packages/test-app build",
    "start:test": "npm run -w packages/test-app start",
    "dev:all": "./dev.sh"
  },
  "keywords": [
    "vite",
    "nextjs",
    "react",
    "cms",
    "blogging",
    "headless-cms",
    "next-js",
    "secure"
  ],
  "author": "Capt ADExp <tech@developer.toys>",
  "license": "ISC",
  "packageManager": "bun@1.0.30"
}